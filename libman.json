{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@7.0.1",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "css/brands.css",
        "css/brands.min.css",
        "css/fontawesome.css",
        "css/fontawesome.min.css",
        "css/regular.css",
        "css/regular.min.css",
        "css/solid.css",
        "css/solid.min.css",
        "css/svg-with-js.css",
        "css/svg-with-js.min.css",
        "css/svg.css",
        "css/svg.min.css",
        "css/v4-font-face.css",
        "css/v4-font-face.min.css",
        "css/v4-shims.css",
        "css/v4-shims.min.css",
        "css/v5-font-face.css",
        "css/v5-font-face.min.css",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-v4compatibility.woff2"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-icons@1.13.1",
      "destination": "wwwroot/lib/bootstrap-icons/"
    },
    {
      "provider": "cdnjs",
      "library": "sweetalert2@11.23.0",
      "destination": "wwwroot/lib/sweetalert2/"
    }
  ]
}